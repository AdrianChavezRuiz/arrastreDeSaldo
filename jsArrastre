
function sumar() {
  var a=  parseFloat(document.getElementById("saldo").value);
  
  var b=  parseFloat(document.getElementById("sabado").value);
  
  var c=  parseFloat(document.getElementById("lunes").value);
  
  var d=  parseFloat(document.getElementById("martes").value);
  
  var e=  parseFloat(document.getElementById("miercoles").value);
  
  var f=  parseFloat(document.getElementById("jueves").value);
  
  var g=  parseFloat(document.getElementById("viernes").value);
  // cargas diarias 
  var h= parseFloat(document.getElementById("transR").value);
  var i= parseFloat(document.getElementById("dtsCob").value);
  var j= parseFloat(document.getElementById("dtsOt").value);
  var k= parseFloat(document.getElementById("devQ").value);
  var l= parseFloat(document.getElementById("devM").value);
  var m= parseFloat(document.getElementById("transEnt").value);
  //dep√≥sitos 
  var n=  parseFloat(document.getElementById("sabado1").value);
  
  var o=  parseFloat(document.getElementById("lunes1").value);
  
  var p=  parseFloat(document.getElementById("martes1").value);
  
  var q=  parseFloat(document.getElementById("miercoles1").value);
  
  var r=  parseFloat(document.getElementById("jueves1").value);
  
  var s=  parseFloat(document.getElementById("viernes1").value);
  
  //depositos
  var t= parseFloat(document.getElementById("bonif").value);
  var u= parseFloat(document.getElementById("neg").value);
  var v= parseFloat(document.getElementById("credOtraPlaza").value);

  var re = a + b + c + d + e + f + g + h + i - j - k - l - m - n - o - p - q - r - s - t - u - v;
document.getElementById("saldoC").innerHTML=(re);

  }

